<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F543959F"
    tools:context=".AdicionarAlimentos">

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adicionar"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="667dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adicionar Alimentos"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="41dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Refeição:"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="113dp" />

    <View
        android:id="@+id/view5"
        android:layout_width="304dp"
        android:layout_height="483dp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="178dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F543959F"
        app:srcCompat="@android:drawable/btn_dialog"
        tools:layout_editor_absoluteX="368dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/DropBox"
        android:layout_width="60dp"
        android:layout_height="48dp"
        android:clickable="true"
        app:srcCompat="@android:drawable/btn_dropdown"
        tools:layout_editor_absoluteX="275dp"
        tools:layout_editor_absoluteY="104dp" />

</androidx.constraintlayout.widget.ConstraintLayout>